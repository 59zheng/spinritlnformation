            <if test="patientId != null ">patient_id = #{patientId},</if>
            <if test="integral != null  and integral != ''">integral = #{integral},</if>
            <if test="centroid != null  and centroid != ''">centroid = #{centroid},</if>
            <if test="slope != null  and slope != ''">slope = #{slope},</if>
            <if test="describe != null  and describe != ''">describe = #{describe},</if>
            <if test="hint != null  and hint != ''">hint = #{hint},</if>
