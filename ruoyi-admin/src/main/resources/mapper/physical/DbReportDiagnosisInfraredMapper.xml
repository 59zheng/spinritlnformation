    </delete>

    <delete id="deleteDbReportDiagnosisInfraredByIds" parameterType="String">
        delete from db_report_diagnosis_infrared where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>